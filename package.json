{
  "name": "api-node",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "start": "node dist/index.js",
    "postinstall": "tsc",
    "production": "./dist/index.js",
    "dev": "ts-node-dev --respawn  src/index.ts",
    "knex:migrate": "knex --knexfile ./src/server/database/knex/Environment.ts migrate:latest",
    "knex:rollback": "npx knex --knexfile ./src/server/database/knex/Environment.ts migrate:rollback",
    "knex:rollback-all": "npx knex --knexfile ./src/server/database/knex/Environment.ts migrate:rollback-all",
    "knex:seed": "npx knex --knexfile ./src/server/database/knex/Environment.ts seed:run"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-zod-safe": "^3.2.0",
    "http-status-codes": "^2.3.0",
    "knex": "^3.1.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^9.39.2",
    "@jest/globals": "^30.2.0",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "jest": "^30.2.0",
    "sqlite3": "^5.1.7",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1"
  },
  "engines": {
    "node": "22.x"
  }
}
